{
  "name": "next-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "xo --dir src",
    "stylelint": "stylelint --ignore-path .gitignore './src/**/*.{css,scss,ts,tsx}'",
    "prettier": "prettier --config .prettierrc.yaml './src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "xo --fix",
    "fix": "yarn lint --fix && yarn stylelint --fix && yarn format",
    "format": "yarn prettier --write",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install",
    "test": "jest",
    "test:com": "jest --silent=false --verbose",
    "test:all:cvg": "jest --silent --config jest.allcv.config.js --coverage --testLocationInResults --ci --all --json --outputFile='report.json'",
    "test:part:cvg": "jest --silent --testLocationInResults --ci --all --coverage --json --outputFile='./partcoverage/report.json'",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.8",
    "@mui/system": "^5.10.8",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "^0.7.5",
    "recoil-persist": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "18.8.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "babel-loader": "^8.2.5",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-xo": "^0.42.0",
    "eslint-config-xo-nextjs": "^4.2.1",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "prettier": "^2.7.1",
    "recoil-mock": "^0.1.4",
    "sass": "^1.55.0",
    "stylelint": "^14.13.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "4.8.4",
    "xo": "^0.52.4"
  }
}
